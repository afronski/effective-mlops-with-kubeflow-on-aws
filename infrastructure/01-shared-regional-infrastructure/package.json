{
  "name": "infrastructure",
  "version": "1.0.0",
  "bin": {
    "infrastructure": "bin/infrastructure.js"
  },
  "scripts": {
    "build": "npx tsc",
    "lint": "npx eslint . --ext .ts",
    "package": "npm run build && npm run lint && npx cdk synth",
    "bootstrap": "npx cdk bootstrap",
    "deploy-shared-infrastructure": "npm run package && npx cdk deploy KubeflowOnAWS-Shared-Infrastructure",
    "deploy": "npm run package && npx cdk deploy --all"
  },
  "devDependencies": {
    "@types/js-yaml": "~4.0.5",
    "@types/node": "~18.0.0",
    "@typescript-eslint/eslint-plugin": "~5.12.0",
    "@typescript-eslint/parser": "~5.12.0",
    "aws-cdk": "2.28.1",
    "eslint": "~8.9.0",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-promise": "~6.0.0",
    "ts-node": "~9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-cloud9-alpha": "2.28.1-alpha.0",
    "aws-cdk-lib": "2.28.1",
    "cdk-iam-floyd": "0.384.0",
    "constructs": "10.1.42",
    "js-yaml": "~4.1.0",
    "source-map-support": "~0.5.16"
  }
}